package com.myspace.mymortgagedemo;

import java.lang.Number;

rule "Validate Application"
	dialect "mvel"
	ruleflow-group "validation"
	when
		app : Application( age < 21 || age >= 65 || income < 21600 )
		error : ErrorMessage( )
	then
		modify( error ) {
				setStatus( "Required Review" ), 
				setMessage( "Please check application data validity. Applicant age must be between 21 and 65. Annual income must be more than 21,600.00" )
		}
end
